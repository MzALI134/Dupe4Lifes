{
  "routes": [
    { "src": "/about", "dest": "/about.html" },
    { "src": "/contact", "dest": "/contact.html" }
  ],
  "headers": [
    { "source": "/(.*)", "headers": [{ "key": "Cache-Control", "value": "max-age=86400" }] }
  ],
  "rewrites": [
    { "source": "/old", "destination": "/new" }
  ],
  "trailingSlash": true
}
